{
  "extends": [
    "msd",
    "msd/es6",
    "msd/react"
  ],
  "plugins": [
    "react"
  ],
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2017,
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  },
  "globals": {
    "__DEV__": false
  },
  "env": {
    "node": true,
    "es6": true,
    "jasmine": true,
    "jest": true
  },
  "rules": {
    "no-use-before-define": ["off"],
    "react/jsx-closing-bracket-location": [1, "line-aligned"],
    "react/require-optimization": ["off"]
  }
}
